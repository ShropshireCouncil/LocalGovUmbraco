{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    }
  },
  "Umbraco": {
    "CMS": {
      "Hosting": {
        "Debug": false
      },
      "Runtime": {
        "Mode": "Production"
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Global": {
        "UseHttps": true,
        "TimeOut": "00:20:00"
      },
      "ModelsBuilder": {
        "ModelsMode": "Nothing"
      },
      "WebRouting": {
        "DisableFindContentByIdPath": true
      }
    }
  }
}
